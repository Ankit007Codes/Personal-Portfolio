{
  "name": "devfolio",
  "version": "2.0.0",
  "description": "Modern Developer Portfolio - RAj Bardhan Singh",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "build:css": "cssnano style.css style.min.css",
    "build:js": "terser script.js -o script.min.js -c -m",
    "build": "npm run build:css && npm run build:js",
    "lighthouse": "lighthouse https://your-portfolio-url.vercel.app/ --view",
    "test:performance": "lighthouse https://your-portfolio-url.vercel.app/ --only-categories=performance --output=json --output-path=./lighthouse-report.json"
  },
  "keywords": [
    "portfolio",
    "web-developer",
    "frontend",
    "gsap",
    "locomotive-scroll"
  ],
  "author": "RAj Bardhan Singh",
  "license": "MIT",
  "devDependencies": {
    "cssnano-cli": "^1.0.5",
    "terser": "^5.16.0",
    "lighthouse": "^10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ankit007Codes/Personal-Portfolio.git"
  },
  "homepage": "https://github.com/Ankit007Codes/Personal-Portfolio"
}
